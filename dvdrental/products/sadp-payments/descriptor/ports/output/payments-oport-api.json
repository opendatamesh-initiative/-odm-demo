{
    "datastoreapi": "1.0.0",
    "info": {
        "title": "Payments",
        "summary": "This API exposes the payment transactions of DVD Renatal",
        "version": "1.0.0",
        "datastoreName": "payments"
    },
    "services": {
        "development": {
            "serverInfo": {
                "$ref": "#components.serverInfo.paymentsStoreServerInfo"
            },
            "serverVariables": {
                "host": "localhost"
            }
        },
        "production": {
            "serverInfo": {
                "$ref": "#components.serverInfo.paymentsStoreServerInfo"
            },
            "serverVariables": {
                "host": "localhost"
            }
        }
    },
    "schema": {
        "databaseName": "dvdrental",
        "schemaName": "dp_payments",
        "tables": [
            {
                "name": "custopaymentsmers",
                "specification": "schemata",
                "specificationVersion": 1,
                "definition": {
                    "mediaType": "application/x-protobuf",
                    "$ref": "payments-oport-schema.proto"
                }
               
            }
        ]
    },
    "components": {
        "serverInfo": {
            "tripStatusStoreServerInfo": {
                "host:": "{host}",
                "port:": "5432",
                "dbmsType:": "Postgres",
                "dbmsVersion:": "15.3",
                "connectionProtocols": {
                    "jdbc": {
                        "version": "1.0",
                        "url": "jdbc:postgresql://{hosts}:5432/dvdrental",
                        "driverName": "PostgreSQL JDBC Driver",
                        "driverClass": "org.postgresql.Driver",
                        "driverVersion": "42.2.20",
                        "driverLibrary": {
                            "description": "PostgreSQL JDBC Driver Library",
                            "dataType": "application/java-archive",
                            "$href": "https://jdbc.postgresql.org/"
                        },
                        "driverDocs": {
                            "description": "PostgreSQL JDBC Driver HomePage",
                            "dataType": "text/html",
                            "$href": "https://jdbc.postgresql.org/postgresql-15RC2.jdbc3.jar"
                        }
                    }
                }
            }
        }
    }
}